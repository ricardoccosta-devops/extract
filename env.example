# Configurações de LLM Providers
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_BASE_URL=https://api.anthropic.com

META_API_KEY=your_meta_api_key_here
META_BASE_URL=https://api.meta.ai/v1

OLLAMA_BASE_URL=http://localhost:11434

AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1
AWS_BEDROCK_ENDPOINT_URL=https://bedrock-runtime.us-east-1.amazonaws.com

# Azure Storage
AZURE_STORAGE_CONNECTION_STRING=your_azure_connection_string_here
AZURE_STORAGE_CONTAINER_NAME=your_container_name

# S3 Configuration
S3_BUCKET_NAME=your_bucket_name_here

# Application Settings
LOG_LEVEL=INFO
MAX_FILE_SIZE_MB=50
CACHE_ENABLED=true

# Paths
DEFAULT_UPLOAD_PATH=./uploads
DEFAULT_OUTPUT_PATH=./outputs

